{
  "kind": "collectionType",
  "collectionName": "posts_post",
  "info": {
    "singularName": "posts",
    "pluralName": "posts-post",
    "displayName": "Posts"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "Title",
      "required": true
    },
    "Description": {
      "type": "richtext"
    },
    "Featured_Video": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "files",
        "videos"
      ]
    },
    "Featured_Image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "posts"
    },
    "Featured_Post": {
      "type": "boolean"
    },
    "author": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::author.author"
    }
  }
}
